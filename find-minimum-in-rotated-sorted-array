class Solution {
public:
    int binarysearchmin(vector<int>& nums, int ans,int low,int high)
    {
        if(low>high)
        return ans;
        else
        {
            int mid=(low+high)/2;
            if(nums[low]<=nums[mid])
            {
                ans=min(nums[low],binarysearchmin(nums,ans,mid+1,high));
            }
            else
            {
                ans = min(nums[mid],binarysearchmin(nums,ans,low,mid-1));
                
            }
            return ans;
        }
    }
    int findMin(vector<int>& nums) {
        int n=nums.size();
        return binarysearchmin(nums,nums[0],0,n-1);
    }
};
