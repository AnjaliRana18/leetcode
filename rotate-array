class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n = nums.size();
        k=k%n;
        int r;
        vector<int> t;
        for(int i=n-k;i<n;i++)
        {
            t.push_back(nums[i]);
        }
        nums.erase(nums.end()-k,nums.end());
        nums.insert(nums.begin(),t.begin(),t.end());
    }
};
